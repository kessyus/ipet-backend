{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": ["error", {"singleQuote": true, "parser": "flow"}],
      "comma-dangle": [0]
    }
}
